{{!-- //////////////////////////////////////Pie Chart//////////////////////////////////// --}}

<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.js"></script>

<canvas id="myChart" style="width:100%;max-width:1600px"></canvas>

<script>
var xValues = ["Pending", "Delivered", "Cancelled"];
var yValues = [{{pendingCount}},{{successCount}}, {{cancelCount}}];
var barColors = [
  "#667799",
  "#7a9460",
  "#dd7788"
];

new Chart("myChart", {
  type: "pie",
  data: {
    labels: xValues,
    datasets: [{
      backgroundColor: barColors,
      data: yValues
    }]
  },
  options: {
    title: {
      display: true,
      is3D:true,
      text: "Order Status Chart"
    }
  }
});
</script>





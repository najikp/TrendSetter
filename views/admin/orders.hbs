        <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>

        <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>

<script>
     async  function shipOrder(id){       
      axios({
        method:'post',
        url:'/admin/shipOrder/'+id
      }).then((response)=>{
        location.reload()
      }).catch((response)=>{
        console.log(response)
      })
    }

    async function deliveryOrder(id){
       axios({
        method:'post',
        url:'/admin/deliveryOrder/'+id
       }).then((response)=>{
         location.reload()
       }).catch((response)=>{
        console.log(response)
       })
    }

 
    async function cancel(id) {

        axios({
            method: 'post',
            url: '/cancelOrder/' + id
        }).then((response) => {


            location.reload()

        }).catch((response) => {
            console.log(response)
        })
    }

    function cancelOrder(id) {
        swal({
            title: "Are you sure?",
            
            icon: "warning",
            buttons: true,
            dangerMode: true,
        })
            .then((willDelete) => {
                if (willDelete) {
                    this.cancel(id)
                    swal("Poof! Order cancelled successfully!", {
                        icon: "success",
                    });
                } else {
                    swal("Order is safe!");
                }
            });
    }

</script>



<div class="row container-fluid">
    <div class="col-12">
        <div class="mt-5">
            <table id="example" class="table table-striped " style="width:100%">
                <thead>
                    <tr>
                        <th>No.</th>
                        <th>Name</th>
                        {{!-- <th>E-mail</th> --}}
                        <th>Product</th>
                        <th>Quantity</th>
                        <th>TotalPrice</th>
                        <th>Payment Method</th>
                        <th>Payment Status</th>

                        <th>Delivery Status</th>
                        <th>Change Status</th>
                        <th>Cancel Order</th>

                        <th>Date Of Order</th>
                    </tr>
                </thead>


                <tbody>
 
                    {{#each orderDetails}}

                    <tr>
                        <td> {{add @index 1}} </td>
                        <td> {{this.deliveryAddress.name}}</td>
                        {{!-- <td> {{this.userId.email}}</td> --}}
                        <td> {{#each this.products}}{{this.productId.productname}}<br><br> {{/each}}</td>
                        <td> {{#each this.products}}{{this.quantity}}<br> <br> {{/each}}</td>
                        <td>{{this.totalAmount}}</td>
                        <td>{{this.paymentMethod}}</td>
                        <td>{{this.deliveryStatus}}</td>

                        <td>{{this.productStatus}}</td>
                        {{#if (eq this.productStatus "Pending")}}
                        {{!-- <td><a class="btn" href="/admin/shipOrder/{{this._id}}"><i class="fa fa-truck" aria-hidden="true" style="font-size:24px"></i></a></td> --}}
                        <td><button  onclick="shipOrder('{{this._id}}')" style="border: 0ch;background-color: transparent;"><i class="fa fa-truck" aria-hidden="true" style="font-size:24px"></i></button></td>
                        {{/if}}
                        {{#if (eq this.productStatus "shipped")}}
                        {{!-- <td><a class="btn " href="/admin/deliveryOrder/{{this._id}}"><i class="fa fa-shopping-bag" aria-hidden="true" style="font-size:24px"></i></a></td> --}}
                       <td><button   onclick="deliveryOrder('{{this._id}}')" style="border: 0ch;background-color: transparent;"><i class="fa fa-shopping-bag" aria-hidden="true" style="font-size:24px"></i></button></td>
                        {{/if}}

                        {{#if (eq this.productStatus "delivered")}}
                        <td><i class="fa fa-check" style="font-size:24px;color: green;"></i></td>
                        {{/if}}

                        {{#if (eq this.productStatus "cancelled")}}
                        <td><i class="fa fa-ban" aria-hidden="true"  style="font-size:24px"></td>
                        {{/if}}

                        {{#if (or (eq this.productStatus "Pending")(eq this.productStatus "shipped"))}}
                        {{!-- <td><a class="btn btn-danger" href="/admin/cancelOrder/{{this._id}}" style="font-size: 10px;">Cancel</a></td> --}}
                        <td><button class="btn btn-danger" onclick="cancelOrder('{{this._id}}')" style="font-size: 10px;">Cancel</button></td>
                        
                        {{/if}}

                        {{#if (eq this.productStatus "delivered")}}
                        <td style="color: green;">Delivered</td>
                        {{/if}}

                        {{#if (eq this.productStatus "cancelled")}}
                        <td><i class="fa fa-ban" aria-hidden="true"  style="font-size:24px"></i></td>
                        {{/if}}

                        <td>{{format this.createdAt}}</td>
                    </tr>

                    {{/each}}
                </tbody>
            </table>





